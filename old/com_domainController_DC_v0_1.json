{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "domainName": {
            "type": "string",
            "defaultValue": "domainName",
            "metadata": {
                "description": "domainName"
            }
        },
        "vnetSiteID": {
            "type": "string",
            "defaultValue": "vnetSiteID",
            "metadata": {
                "description": "vnetSiteID"
            }
        },
        "subnetId": {
            "type": "string",
            "defaultValue": "subnetId",
            "metadata": {
                "description": "subnetId"
            }
        }
    },
    "variables": {
    },
    "resources": [
        {
            "type": "Microsoft.AAD/DomainServices",
            "name": "[parameters('domainName')]",
            "apiVersion": "2017-01-01",
            "location": "[resourceGroup().location]",
            "properties": {
                "domainName": "[parameters('domainName')]",
                "vnetSiteID": "[parameters('vnetSiteID')]",
                "subnetId": "[parameters('subnetId')]"
            },
            "dependsOn": []
        }
    ],
    "outputs": {
        "addsID": {
            "value": "[resourceId('Microsoft.AAD/DomainServices', parameters('domainName'))]",
            "type": "string"
        },
        "dnsServers": {
            "value": "[resourceId('Microsoft.AAD/DomainServices', parameters('domainName')).domainControllerIpAddress]",
            "type": "array"
        }
    }
}